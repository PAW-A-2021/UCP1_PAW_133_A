@model IEnumerable<UCPpraktikum.Models.Admin>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Nickname Admin")
            </th>
            <th>
                @Html.DisplayName("ID Admin")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NicknameAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAdminNavigation.IdApplication)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAdmin"><i class="glyphicon glyphicon-refresh"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdAdmin"><i class="glyphicon glyphicon-menu-hamburger"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdAdmin"><i class="glyphicon glyphicon-trash"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
